{
  "name": "@ideal-postcodes/supported-browsers",
  "version": "1.1.0",
  "description": "Browsers support by ideal-postcodes.co.uk",
  "author": {
    "name": "Ideal Postcodes",
    "email": "support@ideal-postcodes.co.uk",
    "url": "https://ideal-postcodes.co.uk"
  },
  "bugs": {
    "url": "https://github.com/ideal-postcodes/supported-browsers/issues",
    "email": "support@ideal-postcodes.co.uk"
  },
  "homepage": "https://github.com/ideal-postcodes/supported-browsers",
  "repository": {
    "type": "git",
    "url": "https://github.com/ideal-postcodes/supported-browsers.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run lint && NODE_ENV=test nyc mocha",
    "docs": "typedoc --mode file --out docs/ --name 'Ideal Postcodes Browser Support Matrix' --theme minimal --excludePrivate --hideGenerator --excludeExternals lib/",
    "lint": "eslint lib/**/*.ts",
    "build": "npm run lint && tsc",
    "semantic-release": "semantic-release",
    "prepublishOnly": "npm test && npm run build"
  },
  "prettier": {
    "trailingComma": "es5"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "exit": true,
    "fullTrace": true,
    "require": [
      "ts-node/register",
      "source-map-support/register"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "dist/*",
      "**/*.d.ts"
    ],
    "extension": [
      ".ts",
      ".tsx"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "all": true
  },
  "engines": {
    "node": ">=8.0"
  },
  "license": "MIT",
  "files": [
    "dist"
  ],
  "keywords": [
    "API",
    "Interface",
    "UK",
    "Address",
    "Postcode",
    "Browser Matrix",
    "Ideal Postcodes"
  ],
  "release": {
    "extends": "@cablanchard/semantic-release/dist/npm"
  },
  "devDependencies": {
    "@cablanchard/eslint-config": "~1.1.1",
    "@cablanchard/semantic-release": "~1.3.0",
    "@cablanchard/tsconfig": "~1.0.0",
    "@types/chai": "~4.2.0",
    "@types/mocha": "~5.2.5",
    "@typescript-eslint/eslint-plugin": "~2.14.0",
    "@typescript-eslint/eslint-plugin-tslint": "~2.13.0",
    "@typescript-eslint/parser": "~2.14.0",
    "chai": "~4.2.0",
    "eslint": "~6.8.0",
    "mocha": "~6.2.0",
    "nyc": "~15.0.0",
    "prettier": "~1.19.1",
    "semantic-release": "~15.14.0",
    "source-map-support": "~0.5.9",
    "ts-node": "~8.5.0",
    "tslint": "~5.20.0",
    "typedoc": "~0.15.0",
    "typescript": "~3.7.2"
  },
  "dependencies": {}
}
